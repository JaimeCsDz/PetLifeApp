{
  "expo": {
    "name": "PetLife",
    "slug": "PetLife",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Icono.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/perro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "hideExponentText": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
